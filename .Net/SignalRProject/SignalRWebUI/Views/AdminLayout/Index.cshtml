@{
    Layout = null;
}




<!DOCTYPE html>
<html>
@await Component.InvokeAsync("_LayoutHeaderPartialComponent")

<body>
    <div class="wrapper">
        @await Component.InvokeAsync("_LayoutNavBarComponentPartial")

        @await Component.InvokeAsync("_LayoutNavSideComponentPartial")

        <div class="main-panel">
            @RenderBody()
            @await Component.InvokeAsync("_LayoutFooterComponentPartial")
        </div>
    </div>

    <!-- Modal -->
    @await Component.InvokeAsync("_LayoutScriptComponentPartial")
</body>

</html>

